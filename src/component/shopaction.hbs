{{#with service}}
  <div class='card col-12 text-bg-success text-center'>
    <div class='card-body'>
      <h2>{{title}}</h2>
      <p>{{description}}</p>
      {{#each buttons}}
        <button
          class='btn btn-primary text-dark'
          href='{{link}}'
        >{{text}}</button>
      {{/each}}
    </div>
  </div>
{{/with}}

