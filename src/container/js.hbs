{{! 
Це код container файлу для summary сторінки, 
в якому міститься код, котрий буде віддаватися по шляху (PATH) - "/summary"
}}
<head>
  <div>
    <header>
      <h1>{{name}}</h1>
    </header>
  </div>
  <div>
    <em><h3>{{description}}</h3></em>
    {{#with history}}
      <ul>
        <li>{{year}}</li>
        <li>{{founder}}</li>
      </ul>
    {{/with}}
  </div>
</head>
<br />
<hr />
<main>
  <div>
    {{> title data= 'Types' big=true}}
    <ul>
      <li>{{types}}</li>
    </ul>
    <hr>
     {{#with syntax}}
     {{> title data= 'Syntax' big=true}}
      <ul>
        <li>{{variables}}</li>
        <li>{{functions}}</li>
        <li>{{conditionals}}</li>
        <li>{{loops}}</li>
        <li>{{classes}}</li>
      </ul>
    {{/with}}
    <br />
    <hr />
    {{> title data= 'Features' big=true}}
    <div>{{features}}</div>
    <hr />
    {{> title data= 'Libraries' big=true}}
    <div>{{libraries}}</div>
    <hr>
    {{> title data= 'Tools' big=true}}
    <div>{{tools}}</div>
  </div>
</main>
<br>
<br>
<br>
<br>
<hr>
<footer>
    {{> title data= 'Community' big=true}}
    <div>
        {{#each community}}
        <ul>
            <li>{{name}}</li>
            <li>{{type}}</li>
        </ul>
        {{/each}}
    </div>
</footer>

{{!-- 
    Цей код містить лише те, що буде виводитись в {{{body}}} в layout/default.hbs
    Адже наш layout/default.hbs має вже структуру HTML
--}}