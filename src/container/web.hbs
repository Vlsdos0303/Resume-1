{{! 
Це код container файлу для summary сторінки, 
в якому міститься код, котрий буде віддаватися по шляху (PATH) - "/summary"
}}
<header>
  {{#with web}}
    {{#each languages}}
      <h1>{{name}}</h1>
      <h4>{{version}}</h4>
      <em>{{description}}</em>

    {{/each}}
    {{> title data= 'Elements' big=true}}
    {{#each languages}}
      {{#each elements}}
        <ul>
          <li> {{name}}</li>
          <li>{{description}}</li>
        </ul>
        {{#each attributes}}
        <ul>
            <li>{{name}}</li>
            <li>{{description}}</li>
        </ul>
        {{/each}}
      {{/each}}

      {{#each properties}}
      <ul>
        <li>{{name}}</li>
        <li>{{description}}</li>
        {{#each values}}
        <ul>
            <li>{{value}}</li>
            <li>{{description}}</li>
        </ul>
        {{/each}}
      </ul>
      
      {{/each}}

      {{#each functions}}
      <ul>
        <li>{{name}}</li>
        <li>{{description}}</li>
      </ul>
      {{#each parameters}}
      <ul>
        <li>{{name}}</li>
        <li>{{type}}</li>
        <li>{{description}}</li>
      </ul>
      {{/each}}
      {{/each}}
    {{/each}}
  {{/with}}
</header>

